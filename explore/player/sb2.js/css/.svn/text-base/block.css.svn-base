.reporterBase {
	min-height: 14px;
	min-width: 10px;
	display: inline-block;
	border-radius: 30000px;
	padding: 1px 4px;
	color: #FFFFFF;
	font: bold 10px Verdana;
}

.hat {
	background-image:url(../editorimg/blk/hat.png);
	min-height: 35px;
	min-width: 130px;
	display: inline-block;
}

.hate {
	background-image:url(../editorimg/blk/hate.png);
	height: 24px;
	width: 3px;
	display: inline-block;
	vertical-align: bottom;
}

.wrapper {
	background-image: url(../editorimg/blk/wrapper.png);
	background-repeat: repeat-y;
	padding-left: 12px;
	min-height:11px;
}

.wrapperbtm {
	border-image:url(../editorimg/blk/wbtm.png) 3 3 stretch;
	-moz-border-image:url(../editorimg/blk/wbtm.png) 3 3 stretch;
	-o-border-image:url(../editorimg/blk/wbtm.png) 3 3 stretch;
	min-height: 5px;
	min-width: 54px;
	display: inline-block;
}

.wbtmbg {
	background-color: #E7AD21;
	min-height: 4px;
