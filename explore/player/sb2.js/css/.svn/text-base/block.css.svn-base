.wtopedge {
	background-image: url(../editorimg/blk/wtopedge.png);
	position: absolute;
	-webkit-transform: translate(0px, -3px);
	-moz-transform: translate(0px, -3px);
	-o-transform: translate(0px, -3px);
	width: 14px;
	height: 5px;
}

.wbtmedge {
	background-image: url(../editorimg/blk/wbtmedge.png);
	position: absolute;
	-webkit-transform: translate(0px, -2px);
	-moz-transform: translate(0px, -2px);
	-o-transform: translate(0px, -2px);
	width: 14px;
	height: 3px;
}

.scratchInput {
	font: 10px Verdana;
	padding: 0 3px;
	border-radius: 5px;
	border-style: solid;
	border-width: 1px;
	border-color: gray;
	border-bottom: none;
	min-width: 10px;
	width: 25px;
}

.dropArrow {
	//position: absolute;
	margin-left: -11px;
	margin-right: 4px;
	margin-top: -2px;
	position: relative;
	top: 3px;
}

.lockDrop {
	background-color: rgba(128,128,128,0.3);
	font: 10px Verdana;
	padding: 0 3px;
	border-style: solid;
	border-width: 1px;
	border-top-color: rgba(0,0,0,0.5);
	border-left-color: rgba(0,0,0,0.5);
	border-bottom-color: rgba(255,255,255,0.4);
	border-right-color: rgba(255,255,255,0.4);
	padding-right: 12px;
	min-width: 10px;
	display: inline-block;
	margin-bottom: -1px;
}

.colorBox {
	border-style: solid;
	border-width: 1px;
	border-top-color: rgba(0,0,0,0.5);
	border-left-color: rgba(0,0,0,0.5);
	border-bottom-color: rgba(255,255,255,0.4);
	border-right-color: rgba(255,255,255,0.4);
	width: 10px;
	height: 10px;
	display: inline-block;
	margin-bottom: -2px;
	margin-top: 1px;
}

.emptyIf {
	display: inline-block;
	margin-left: 15px;
	background-image: url(../editorimg/blk/ifhole.png);
	width: 25px;
	height: 13px;
	vertical-align: -2px;
}

.gfimg {
	vertical-align: middle;
